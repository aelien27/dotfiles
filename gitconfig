[user]
  name = Chris Dolan
  email = cdolan@attinteractive.com
[apply]
  whitespace = nowarn
[color]
  ui = auto
  diff = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[github]
  user = aelien27
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  rb = rebase
  spull = !git-svn fetch && git-svn rebase
  spush = !git-svn dcommit
  rdiff = !git fetch origin master --quiet && git log origin/master..master --format=oneline
[core]
	excludesfile = ~/.cvsignore
[push]
	default = simple
